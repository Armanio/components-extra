{
  "name": "components-extra",
  "version": "2.6.0",
  "description": "React UI molecule components based on styled-components & material-ui.",
  "repository": {
    "type": "git",
    "url": "https://github.com/alexandre-lelain/components-extra.git"
  },
  "homepage": "https://components-extra.netlify.com/",
  "author": "Alexandre Le Lain <lelain.alexandre@gmail.com>",
  "license": "MIT",
  "keywords": [
    "react",
    "react-components",
    "material design",
    "molecule components"
  ],
  "module": "./build/esm/index.js",
  "main": "./build/index.js",
  "sideEffects": false,
  "scripts": {
    "start": "cross-env BABEL_ENV=dev docz dev -p 4242",
    "serve": "cross-env BABEL_ENV=dev docz serve docs-build",
    "build": "rimraf build && yarn build:cjs && yarn build:esm && yarn build:es && yarn build:umd && yarn build:after",
    "build:umd": "cross-env NODE_ENV=production BABEL_ENV=production-umd rollup -c",
    "build:cjs": "cross-env NODE_ENV=production BABEL_ENV=cjs babel --config-file ./babel.config.js ./src --out-dir ./build",
    "build:esm": "cross-env NODE_ENV=production BABEL_ENV=esm babel --config-file ./babel.config.js ./src --out-dir ./build/esm",
    "build:es": "cross-env NODE_ENV=production BABEL_ENV=es babel --config-file ./babel.config.js ./src --out-dir ./build/es",
    "build:after": "node ./scripts/afterBuild.js",
    "build:docs": "rimraf docs-build .docz && cross-env BABEL_ENV=dev docz build",
    "validate": "yarn lint && yarn test && yarn build",
    "lint": "eslint --cache src/**/*.js",
    "prettier": "prettier --write {src,docs}/**/*.js",
    "test": "cross-env BABEL_ENV=dev jest",
    "preversion": "yarn validate",
    "postversion": "git push && git push --tags",
    "version": "yarn prettier && git add -vA src",
    "release": "yarn build && yarn publish build --tag latest"
  },
  "dependencies": {
    "@babel/runtime": "^7.8.7",
    "prop-types": "^15.7.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/plugin-transform-object-assign": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "@material-ui/core": "^4.9.7",
    "@rollup/plugin-alias": "^3.0.1",
    "@rollup/plugin-replace": "^2.3.1",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^10.0.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-react-remove-properties": "^0.3.0",
    "babel-plugin-transform-imports": "^2.0.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "cross-env": "^7.0.2",
    "docz": "^2.2.0",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "fs-extra": "^8.1.0",
    "gatsby-plugin-alias-imports": "^1.0.5",
    "gatsby-plugin-manifest": "^2.2.48",
    "gatsby-plugin-material-ui": "^2.1.6",
    "gatsby-plugin-offline": "^3.0.42",
    "gatsby-plugin-styled-components": "^3.1.21",
    "gatsby-theme-docz": "^2.2.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-is": "^16.13.0",
    "react-json-view": "^1.19.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.1.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-externals": "^2.1.3",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.2",
    "rollup-plugin-size-snapshot": "^0.11.0",
    "rollup-plugin-terser": "^5.3.0",
    "scroll-behavior-polyfill": "^2.0.13",
    "styled-components": "^5.0.1"
  },
  "peerDependencies": {
    "@material-ui/core": "^4.9.7",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "styled-components": "^5.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --pattern \"{src,docs}/**/*.js\""
    }
  }
}
