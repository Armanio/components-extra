{
  "name": "components-extra",
  "version": "2.6.1",
  "description": "React UI molecule components based on styled-components & material-ui.",
  "homepage": "https://components-extra.netlify.com/",
  "author": "Alexandre Le Lain <lelain.alexandre@gmail.com>",
  "license": "MIT",
  "module": "./build/esm/index.js",
  "main": "./build/index.js",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/alexandre-lelain/components-extra.git"
  },
  "keywords": [
    "react",
    "react-components",
    "material design",
    "molecule components"
  ],
  "scripts": {
    "build:tsc": "rimraf build-tsc && tsc -m es6 --outDir build-tsc/esm",
    "build": "rimraf build && yarn build:cjs && yarn build:esm && yarn build:es && yarn build:umd && yarn build:after",
    "build:umd": "cross-env NODE_ENV=production BABEL_ENV=production-umd rollup -c",
    "build:cjs": "cross-env NODE_ENV=production BABEL_ENV=cjs babel --config-file ./babel.config.js ./src --out-dir ./build",
    "build:esm": "cross-env NODE_ENV=production BABEL_ENV=esm babel --config-file ./babel.config.js ./src --out-dir ./build/esm",
    "build:es": "cross-env NODE_ENV=production BABEL_ENV=es babel --config-file ./babel.config.js ./src --out-dir ./build/es",
    "build:after": "node ./scripts/afterBuild.js",
    "test": "cross-env BABEL_ENV=dev jest",
    "postversion": "git push && git push --tags",
    "version": "git add -vA src",
    "release": "yarn build && yarn publish build --tag latest"
  },
  "dependencies": {
    "@babel/runtime": "^7.9.2",
    "prop-types": "^15.7.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-object-assign": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@material-ui/core": "^4.9.9",
    "@rollup/plugin-alias": "^3.0.1",
    "@rollup/plugin-replace": "^2.3.1",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.3.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-imports": "^2.0.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "cross-env": "^7.0.2",
    "fs-extra": "^9.0.0",
    "jest": "^25.2.6",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-is": "^16.13.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.3.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-externals": "^2.1.3",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.2",
    "rollup-plugin-size-snapshot": "^0.11.0",
    "rollup-plugin-terser": "^5.3.0",
    "typescript": "^3.8.3"
  },
  "peerDependencies": {
    "@material-ui/core": "^4.9.7",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "styled-components": "^5.0.1"
  }
}
